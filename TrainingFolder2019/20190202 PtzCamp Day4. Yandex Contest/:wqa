#include <bits/stdc++.h>
using namespace std;

int qcnt = 0;
int query(char c){
	qcnt++;
	cout << c << endl;
	return 0;
//	int x; cin >> x; return x;
}

vector<int> catch_seq = {7, 28, 80, 1000};

void conj(string s){
	query('+');
	if(query('-')){
		cout << "! ugly" << endl;
		exit(0);
	}
	else{
		cout << "! " + s << endl;
		exit(0);
	}
}
int main(){
	int deviate = 0;
	int prv = 0; 
	for(auto &i : catch_seq){
		for(int j=0; j<i+deviate; j++){
			int c = query('-');
			if(c == 1) conj("good");
		}
		for(int j=0; j<(i+deviate)*3; j++){
			int c = query('+');
			if(c == 1) conj("bad");
		}
		if(i == 112) break;
		for(int j=0; j<2*(i+deviate); j++){
			int c = query('-');
			if(c == 1) conj("good");
		}
		deviate += (i + deviate) * 3;
	}
	cout << "qcnt = " << qcnt << endl;
	cout << "! ugly" << endl;
	return 0;
}
